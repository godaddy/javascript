    "biome-config-godaddy": "workspace:*",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "tsdown": "^0.15.6",
    "typescript": "~5.7.3",
    "vite": "^5.1.6",
    "vitest": "^3.0.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
