{
  "name": "eslint-config-godaddy-flow",
  "version": "5.0.0",
  "description": "ESLint config for consistent style in node projects using Flow at GoDaddy.",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run --silent lint",
    "test": "echo ok",
    "clean": "rimraf node_modules",
    "unused": "eslint-find-rules --unused ./index.js || echo ''"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/javascript.git"
  },
  "keywords": [
    "godaddy",
    "javascript",
    "styleguide",
    "style-guide",
    "eslint",
    "es6",
    "flow"
  ],
  "bin": {
    "eslint-godaddy-flow": "bin/eslint-godaddy-flow"
  },
  "peerDependencies": {
    "@babel/core": ">=7",
    "eslint": ">=7"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.16.5",
    "eslint-config-godaddy": "^6.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^8.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "eslint": "^8.4.1"
  }
}
