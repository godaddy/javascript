{
  "name": "eslint-config-godaddy",
  "version": "7.1.1",
  "description": "Base ESLint config for consistent style in ES6 projects at GoDaddy.",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run --silent lint",
    "test": "echo ok",
    "clean": "rimraf node_modules",
    "unused": "eslint-find-rules --unused ./index.js || echo ''"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:godaddy/javascript.git"
  },
  "keywords": [
    "godaddy",
    "javascript",
    "styleguide",
    "style-guide",
    "eslint",
    "es6"
  ],
  "bin": {
    "eslint-godaddy": "bin/eslint-godaddy"
  },
  "peerDependencies": {
    "eslint": "^8"
  },
  "dependencies": {
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-jsdoc": "^48.11.0"
  },
  "devDependencies": {
    "eslint": "^9.17.0"
  }
}
